@model SaleMonitoring.Models.Manager.ManagerViewModel

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
<div class="panel-success">
    <div class="form-group">
        <label>Имя менеджера</label>
        <h2>@Model.ManagerName</h2>
    </div>
    <div class="form-group">
        <label>Телефон менеджера</label>
        <h2>@Model.ManagerTelephone</h2>
    </div>
    <div class="form-group">
        <label>Ранг менеджера</label>
        <h2>@Model.ManagerRank</h2>
    </div>
    <div class="form-group">
        <label>Дата</label>
        <h2>@Model.Date</h2>
    </div>
</div>

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
</body>
</html>